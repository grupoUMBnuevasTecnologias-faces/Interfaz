<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <managed-bean>
        <description>Procesos</description>
        <managed-bean-name>proceso</managed-bean-name>
        <managed-bean-class>com.davivienda.procesos.Proceso</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <description>GoogleMaps</description>
        <managed-bean-name>googleMaps</managed-bean-name>
        <managed-bean-class>com.google.maps.GoogleMaps</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <description>Mail</description>
        <managed-bean-name>mail</managed-bean-name>
        <managed-bean-class>com.davivienda.gmail.Mail</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>
